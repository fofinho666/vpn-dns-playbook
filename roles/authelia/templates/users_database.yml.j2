###############################################################
#                         Users Database                      #
###############################################################

# This file can be used if you do not have an LDAP set up.

# List of users
users:
  {{ user }}:
    displayname: "{{ user }}"
    password: "{{ authelia_password | string | password_hash('sha512') }}"
    email: email@localhost
    groups:
      - admins
      - dev